<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window id="501">
    <defaultcontrol always="true">50</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
    <views>50</views>
    <controls>
        <!--Global Background-->
        <include>GlobalBackground</include>
        <!--Time & Weather-->
        <include>TimeWeather</include>
        <!--Header-->
        <include>Header</include>
        <!--Music_NowPlaying-->
        <include>NowPlayingMusic</include>
        <!--Video_NowPlaying-->
        <include>NowPlayingVideo</include>
        <!--Hidden Button-->
        <control type="radiobutton" id="19">
            <description>Filter (hidden)</description>
            <include>HiddenButton</include>
        </control>
        <!--Views-->
        <include>View_50_List</include>
        <!--Sub Menu Tab Left-->
        <include>SubMenuTabLeft</include>
        <!--Sub Menu-->
        <control type="group">
            <posx>10</posx>
            <posy>100</posy>
            <visible>!Window.IsActive(DialogKeyboard.xml)</visible>
            <include>animation_fade_visible_hidden</include>
            <include>animation_fade_open_close</include>
            <include>animation_sub_menu</include>
            <!--Panel-->
            <control type="image">
                <posx>-13</posx>
                <posy>240</posy>
                <width>40</width>
                <height>53</height>
                <texture>dialogs/default/arrow-left-black.png</texture>
            </control>
            <control type="image">
                <width>333</width>
                <height>533</height>
                <texture border="39">dialogs/default/bg-black.png</texture>
                <aspectratio>stretch</aspectratio>
            </control>
            <!--Gouplist 9000-->
            <control type="grouplist" id="9000">
                <posx>14</posx>
                <posy>47</posy>
                <width>305</width>
                <height>433</height>
                <itemgap>2</itemgap>
                <onleft>50</onleft>
                <onright>50</onright>
                <onup>9000</onup>
                <ondown>9000</ondown>
                <orientation>vertical</orientation>
                <!--Buttons-->
                <control type="label" id="200">
                    <!--Label Menu-->
                    <width>305</width>
                    <align>left</align>
                    <textoffsetx>33</textoffsetx>
                    <font>Light40</font>
                    <textcolor>mainblue</textcolor>
                    <label>$LOCALIZE[41090]:</label>
                </control>
                <control type="image" id="201">
                    <!--Seperator-->
                    <width>305</width>
                    <height>13</height>
                    <align>left</align>
                    <texture>common/menu-separator.png</texture>
                </control>
                <control type="button" id="3">
                    <!--Button Sort-->
                    <include>SubMenuButton</include>
                </control>
                <control type="togglebutton" id="4">
                    <!--Button Ascending-->
                    <include>SubMenuButton</include>
                    <label>584</label>
                    <altlabel>585</altlabel>
                </control>
                <control type="button" id="99">
                    <!--Button Library-->
                    <include>SubMenuButton</include>
                    <label>29800</label>
                    <onclick>ReplaceWindow(MusicLibrary)</onclick>
                    <onclick>SetFocus(50)</onclick>
                    <enable>Library.HasContent(Music)</enable>
                </control>
                <control type="button" id="7">
                    <!--Button Playlist-->
                    <include>SubMenuButton</include>
                    <label>136</label>
                </control>
                <control type="button" id="11">
                    <!--Button Search-->
                    <include>SubMenuButton</include>
                    <label>600</label>
                </control>
                <control type="image" id="203">
                    <!--Space-->
                    <width>305</width>
                    <height>13</height>
                    <align>left</align>
                    <texture>common/menu-space.png</texture>
                </control>
                <control type="label" id="300">
                    <!--View:-->
                    <width>305</width>
                    <align>left</align>
                    <textoffsetx>33</textoffsetx>
                    <font>Light40</font>
                    <textcolor>mainblue</textcolor>
                    <label>$LOCALIZE[457]</label>
                </control>
                <control type="image" id="301">
                    <!--Seperator-->
                    <width>305</width>
                    <height>13</height>
                    <align>left</align>
                    <texture>common/menu-separator.png</texture>
                </control>
                <control type="button" id="302">
                    <!--View List-->
                    <include>SubMenuButton</include>
                    <label>List</label>
                    <onclick>Container.SetViewMode(50)</onclick>
                    <onfocus>Skin.Reset(OtherView)</onfocus>
                </control>
            </control>
        </control>
        <!--Youtube Search Overlay-->
        <include>YoutubeSearchOverlay</include>
    </controls>
</window>