<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window>
    <defaultcontrol always="true">450</defaultcontrol>
    <controls>
        <!--Dialog Overlay-->
        <include>DialogOverlay</include>
        <!--Dialog File Stacking-->
        <control type="group">
            <include>animation_dialog</include>
            <control type="image">
                <posx>613</posx>
                <posy>264</posy>
                <width>53</width>
                <height>40</height>
                <texture>dialogs/default/arrow.png</texture>
            </control>
            <control type="image">
                <posx>469</posx>
                <posy>277</posy>
                <width>341</width>
                <height>165</height>
                <texture border="39">dialogs/default/bg.png</texture>
            </control>
            <control type="label">
                <description>Heading</description>
                <posx>527</posx>
                <posy>233</posy>
                <width>225</width>
                <label>$INFO[ListItem.Label]</label>
                <include>DialogHeader</include>
                <align>center</align>
            </control>
            <control type="group">
                <posx>492</posx>
                <posy>300</posy>
                <control type="image">
                    <posx>216</posx>
                    <posy>0</posy>
                    <width>80</width>
                    <height>121</height>
                    <texture border="5">dialogs/default/inner.png</texture>
                </control>
                <control type="image">
                    <description>Poster</description>
                    <posx>217</posx>
                    <posy>1</posy>
                    <width>79</width>
                    <height>119</height>
                    <texture>$INFO[ListItem.Icon]</texture>
                    <aspectratio>scale</aspectratio>
                </control>
                <control type="image">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>207</width>
                    <height>121</height>
                    <texture border="5">dialogs/default/inner.png</texture>
                </control>
                <control type="list" id="450">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>207</width>
                    <height>120</height>
                    <onleft>63</onleft>
                    <onright>63</onright>
                    <onup>450</onup>
                    <ondown>450</ondown>
                    <scrolltime tween="cubic" easing="out">500</scrolltime>
                    <pagecontrol></pagecontrol>
                    <itemlayout height="40">
                        <control type="image">
                            <posx>0</posx>
                            <posy>1</posy>
                            <width>180</width>
                            <height>40</height>
                            <texture>dialogs/default/separator.png</texture>
                        </control>
                        <control type="label">
                            <posx>13</posx>
                            <posy>0</posy>
                            <width>180</width>
                            <height>40</height>
                            <info>ListItem.Label</info>
                            <align>left</align>
                            <aligny>center</aligny>
                            <include>DialogListNF</include>
                            <scroll>false</scroll>
                        </control>
                    </itemlayout>
                    <focusedlayout height="40">
                        <control type="group">
                            <visible>!Control.HasFocus(450)</visible>
                            <animation effect="fade" start="0" end="100" time="300" tween="sine" easing="inout" condition="![Container(450).Scrolling | Container(450).OnScrollNext | Container(450).OnScrollPrevious]">Visible</animation>
                            <animation effect="fade" start="100" end="0" time="300" tween="sine" easing="inout" condition="![Container(450).Scrolling | Container(450).OnScrollNext | Container(450).OnScrollPrevious]">Hidden</animation>
                            <control type="image">
                                <posx>0</posx>
                                <posy>1</posy>
                                <width>180</width>
                                <height>40</height>
                                <texture>dialogs/default/separator.png</texture>
                            </control>
                            <control type="label">
                                <posx>13</posx>
                                <posy>0</posy>
                                <width>180</width>
                                <height>40</height>
                                <info>ListItem.Label</info>
                                <align>left</align>
                                <aligny>center</aligny>
                                <include>DialogListNF</include>
                                <scroll>false</scroll>
                            </control>
                        </control>
                        <control type="group">
                            <visible>Control.HasFocus(450)</visible>
                            <animation effect="fade" start="0" end="100" time="300" tween="sine" easing="inout" condition="![Container(450).Scrolling | Container(450).OnScrollNext | Container(450).OnScrollPrevious]">Visible</animation>
                            <animation effect="fade" start="100" end="0" time="300" tween="sine" easing="inout" condition="![Container(450).Scrolling | Container(450).OnScrollNext | Container(450).OnScrollPrevious]">Hidden</animation>
                            <control type="image">
                                <width>207</width>
                                <height>40</height>
                                <texture border="2">dialogs/default/highlight.png</texture>
                            </control>
                            <control type="image">
                                <posx>164</posx>
                                <width>43</width>
                                <height>40</height>
                                <texture border="2">dialogs/filestacking/play.png</texture>
                            </control>
                            <control type="label">
                                <posx>13</posx>
                                <posy>0</posy>
                                <width>151</width>
                                <height>40</height>
                                <info>ListItem.Label</info>
                                <include>DialogListFO</include>
                                <align>left</align>
                                <aligny>center</aligny>
                                <scrollout>false</scrollout>
                                <pauseatend>200</pauseatend>
                                <scrollspeed>20</scrollspeed>
                            </control>
                        </control>
                    </focusedlayout>
                </control>
                <control type="image" id="62">
                    <animation effect="fade" start="100" end="0" time="0" delay="1500">WindowOpen</animation>
                </control>
                <control type="image" id="61">
                    <animation effect="fade" start="0" end="100" time="0">Visible</animation>
                    <animation effect="fade" start="100" end="0" time="0">Hidden</animation>
                    <visible>Control.HasFocus(60) | !Control.IsVisible(62)</visible>
                </control>
                <control type="image" id="61">
                    <animation effect="fade" start="0" end="100" time="0">Visible</animation>
                    <animation effect="fade" start="100" end="0" time="0" delay="1000">Hidden</animation>
                    <visible>!Control.HasFocus(60) + [Container(450).Scrolling | Container(450).OnScrollNext | Container(450).OnScrollPrevious]</visible>
                </control>
            </control>
            <control type="button" id="63">
                <description>Close</description>
                <posx>483</posx>
                <posy>416</posy>
                <label>$LOCALIZE[222]</label>
                <include>DialogButtonOKCancel</include>
                <texturefocus>common/cancel-fo.png</texturefocus>
                <texturenofocus>common/cancel-nf.png</texturenofocus>
                <width>233</width>
                <height>67</height>
                <onclick>PreviousMenu</onclick>
                <onleft>450</onleft>
                <onright>450</onright>
                <onup>450</onup>
                <ondown>450</ondown>
            </control>
        </control>
    </controls>
</window>