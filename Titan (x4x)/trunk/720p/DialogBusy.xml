<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window id="136">
    <defaultcontrol always="true">-</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
    <controls>
        <control type="group">
            <visible>!Window.IsActive(movieinformation)</visible>
            <!--DialogVideoInfo play trailer-->
            <posx>640</posx>
            <!--Center-->
            <posy>360</posy>
            <!--Center-->
            <control type="multiimage">
                <animation effect="fade" start="0" end="100" time="0">WindowOpen</animation>
                <animation effect="fade" start="100" end="0" time="500" delay="500">WindowClose</animation>
                <posx>-27</posx>
                <posy>-27</posy>
                <width>54</width>
                <height>53</height>
                <imagepath>dialogs/busy</imagepath>
                <timeperimage>100</timeperimage>
                <colordiffuse></colordiffuse>
                <fadetime>0</fadetime>
            </control>
        </control>
    </controls>
</window>